{
  "manifest_version": 2,
  "name": "Cleanse",
  "description": "Make annoying websites less annoying",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "storage", "notifications"]
}
